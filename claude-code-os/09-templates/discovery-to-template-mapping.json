  "extraction_patterns": {
    "constant": {
      "description": "Use a predefined constant value",
      "implementation": "Return hardcoded value from config"
    },
    "current_date": {
      "description": "Generate current date in specified format",
      "implementation": "Format Date.now() as 'Month YYYY'"
    },
    "direct_quote": {
      "description": "Extract exact text from specified section",
      "implementation": "Locate section header, extract first 2-N words or sentence"
    },
    "synthesized_summary": {
      "description": "Read full section and create summary",
      "implementation": "Read section, identify key components, synthesize within word count"
    },
    "quantified_metric": {
      "description": "Extract numerical values with context",
      "implementation": "Search for numbers, percentages, currency, durations - extract with units"
    },
    "list_aggregation": {
      "description": "Combine multiple sources into bulleted list",
      "implementation": "Scan multiple sections, aggregate into 5-7 items, format as markdown"
    },
    "calculated_value": {
      "description": "Compute value from multiple sources",
      "implementation": "Extract base values, apply formula, format result"
    },
    "risk_to_question": {
      "description": "Convert risk statement into question",
      "implementation": "Find risk in table/list, rephrase as 'What if...' question"
    },
    "memorable_moment": {
      "description": "Find emotional or breakthrough moment in transcripts",
      "implementation": "Search for exclamations, explicit excitement, 'aha moments', strong quotes"
    }
  }
